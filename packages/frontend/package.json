  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.10",
    "@types/umami": "^2.10.0",
    "beasties-webpack-plugin": "^0.1.0",
    "global-jsdom": "^25.0.0",
