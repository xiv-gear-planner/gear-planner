{
  "name": "gearplan-frontend",
  "version": "1.0.0",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "xivmath": "1.0.0"
  },
  "exports": {
    "exports": {
      "./*.ts": "./src/scripts/*.ts"
    }
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/jsdom": "^21.1.3",
    "@types/mocha": "^10.0.2",
    "global-jsdom": "24.0.0",
    "html-webpack-plugin": "^5.5.3",
    "isomorphic-fetch": "^3.0.0",
    "jsdom": "24.0.0",
    "less": "^4.1.3",
    "mocha": "^10.4.0",
    "ts-loader": "^9.4.4",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.13",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "build": "webpack --mode=development",
    "buildprod": "webpack --mode=production",
    "less": "lessc --source-map ./src/style.less dist/style.css",
    "docs": "typedoc --readme API_DOC.md --out dist/docs --entryPointStrategy expand src/scripts/",
    "test": "ts-mocha src/scripts/test/**/*test.ts src/scripts/test/**/*tests.ts"
  },
  "browser": {
    "[module-name]": false
  }
