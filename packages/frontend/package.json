    "typedoc": "^0.28.4",
    "webpack": "^5.99.8",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "build": "tsc --build && lessc --source-map ./src/style.less dist/style.css && webpack --mode=development",
